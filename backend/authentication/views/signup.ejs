
<% var title = 'Authentication Example' %>
<% include head %>

<h1>Sign up</h1>

<%- message %>
<form method="post" action="/signup">
    <p>
        <label>Username:</label>
        <input type="text" name="username">
    </p>
    <p>
        <label>Password:</label>
        <input type="text" name="password">
    </p>
    <p>
        <input type="submit" value="Sign up">
    </p>
</form>

<% include foot %>
